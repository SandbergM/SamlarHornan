t/UserContext\";\r\nimport NewComment from \"../components/Comment/NewComment\";\r\n\r\nconst ThreadPage = () => {\r\n  const [thread, setThread] = useState(null);\r\n  const [comments, setComments] = useState(null);\r\n\r\n  let { isAdmin, permissions, user } = useContext(UserContext);\r\n  let { threadId, forumUrl } = useParams();\r\n\r\n  const fetchComments = async () => {\r\n    let comments = await fetch(`/api/v1/comments?threadId=${threadId}`);\r\n    switch (comments.status) {\r\n      case 200:\r\n        setComments(await comments.json());\r\n        break;\r\n      default:\r\n        setComments(null);\r\n    }\r\n  };\r\n\r\n  const fetchThreadDetails = async () => {\r\n    let thread = await fetch(\r\n      `/api/v1/threads?id=${threadId}&forumUrl=${forumUrl}`\r\n    );\r\n    switch (thread.status) {\r\n      case 200:\r\n        thread = await thread.json();\r\n        setThread({\r\n          ...thread[0],\r\n          isLocked: thread[0].isLocked ? true : false,\r\n        });\r\n        break;\r\n      default:\r\n        setComments(null);\r\n    }\r\n  };\r\n\r\n  const deleteComment = async (id) => {\r\n    await fetch(`/api/v1/comments/${id}`, {\r\n      method: \"DELETE\",\r\n    });\r\n    let updatedArr = comments.filter((comment) => {\r\n      if (comment.id !== id) return comments;\r\n    });\r\n    setComments([...updatedArr]);\r\n  };\r\n\r\n  const appendComment = (comment) => {\r\n    setComments([...(comments || []), { ...comment, sender: user }]);\r\n  };\r\n\r\n  const lockThread = async () => {\r\n    let res = await fetch(`/api/v1/threads/${threadId}`, {\r\n      method: \"PUT\",\r\n      body: JSON.stringify({ isLocked: !thread.isLocked }),\r\n      headers: { \"Content-type\": \"application/json;charset=utf-8\" },\r\n    });\r\n    if (res.ok) {\r\n      setThread({ ...thread, isLocked: !thread.isLocked });\r\n    }\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetchThreadDetails();\r\n    fetchComments();\r\n  }, [forumUrl]);\r\n\r\n  return (\r\n    <div className=\"d-flex justify-content-around\">\r\n      {thread && (\r\n        <div className=\"col-12\">\r\n          <div className=\"row\">\r\n            <div\r\n              className={`primary-tc secondary-bgc bold oblique p-2 pl-5 d-flex ${\r\n                isAdmin || permissions[forumUrl]\r\n                  ? \"col-10 rounder-upper-left\"\r\n                  : \"col-12 rounded-upper\"\r\n              }`}\r\n            >\r\n              {thread && <h5>{thread.title}</h5>}\r\n              {thread.isLocked ? <h5 className=\"ml-5\">*Låst*</h5> : \"\"}\r\n            </div>\r\n            {(isAdmin || permissions[forumUrl]) && (\r\n              <div className=\"col-2 primary-tc secondary-bgc bold oblique rounder-upper-right d-flex justify-content-center align-items-center\">\r\n                {thread.isLocked ? (\r\n                  <MdLockOutline\r\n                    className=\"moderator-icon pointer\"\r\n                    onClick={() => {\r\n                      lockThread();\r\n                    }}\r\n                  />\r\n                ) : (\r\n                  <MdLockOpen\r\n                    className=\"moderator-icon pointer\"\r\n                    onClick={() => {\r\n                      lockThread();\r\n                    }}\r\n                  />\r\n                )}\r\n              </div>\r\n            )}\r\n            <div className=\"col-12 primary-tc secondary-bgc bold p-2 pl-5 \">\r\n              {thread && <h5>{thread.message}</h5>}\r\n            </div>\r\n            <div className=\"col-12 primary-tc secondary-bgc bold p-2 pl-5 \">\r\n              <div className=\"row\">\r\n                <h5 className=\"col-8\">{convertTimeStamp(thread.published)}</h5>\r\n                {user && !thread.isLocked && (\r\n                  <div className=\"col-4\">\r\n                    <NewComment\r\n                      threadId={threadId}\r\n                      isModerator={isAdmin || permissions[forumUrl]}\r\n                      appendComment={appendComment}\r\n                    />\r\n                  </div>\r\n                )}\r\n              </div>\r\n            </div>\r\n            <div className=\"col-12\">\r\n              <CommentTable deleteComment={deleteComment} comments={comments} />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ThreadPage;\r\n","D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Forum\\ForumTable.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Forum\\ForumRow.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\InputFields\\SearchField.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Header\\Header.js",["128","129"],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\InputFields\\Select.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Thread\\ThreadTable.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Thread\\ThreadRow.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Thread\\NewThread.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Authentication\\AuthenticationModal.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Authentication\\Login.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Authentication\\Register.js",["130"],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\utils\\TimeStampToDateAndTime.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Comment\\CommentTable.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Comment\\CommentRow.js",[],"D:\\Git\\SportsAssociation-Forum\\frontend\\src\\components\\Comment\\NewComment.js",["131"],"import React, { useState, useEffect } from \"react\";\r\nimport {\r\n  Button,\r\n  Modal,\r\n  ModalBody,\r\n  Form,\r\n  FormGroup,\r\n  Label,\r\n  Input,\r\n} from \"r